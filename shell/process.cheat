% ps: process management
# List all running processes
ps -ef

# Find process by name
ps -ef | grep <process_name>

# Find process by user
ps -ef | grep <user_name>

% task manager

# Kill process by name
pkill <process_name>
$process_name: ps -u $USER --- --column 8 --header-lines 1 --preview 'echo {}'

# Kill process by pid
kill <pid>
$process_name: ps -u $USER --- --column 3 --header-lines 1 --preview 'echo {}'